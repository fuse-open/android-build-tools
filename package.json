{
  "name": "android-build-tools",
  "version": "1.6.0",
  "description": "Android SDK and NDK installer for Uno and Fuse apps.",
  "dependencies": {
    "xbash": "^1.6.0"
  },
  "devDependencies": {
    "crlf": "^1.1.1"
  },
  "scripts": {
    "install": "xbash src/install.bash",
    "prepack": "crlf --set=LF src/install.bash"
  },
  "files": [
    "src/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuse-open/android-build-tools.git"
  },
  "keywords": [
    "android",
    "sdk",
    "ndk",
    "installer",
    "uno",
    "fuse"
  ],
  "author": "Morten Daniel Fornes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fuse-open/android-build-tools/issues"
  },
  "homepage": "https://github.com/fuse-open/android-build-tools#readme"
}
